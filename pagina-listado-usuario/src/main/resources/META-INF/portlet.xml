<?xml version="1.0" encoding="UTF-8"?>
<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0">

    <portlet>
        <portlet-name>ListadoDeUsuarios</portlet-name>
        <display-name>Listado De Usuarios</display-name>
        <portlet-class>com.example.ListadoDeUsuariosPortlet</portlet-class>
        <init-param>
            <name>view-template</name>
            <value>/view.jsp</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Listado De Usuarios</title>
        </portlet-info>
    </portlet>

</portlet-app>